def Repeate(x):
  size = len(x)
  repeate = []
  for i in range(size):
    for j in range(i+1,size):
      if x[i] == x[j] and x[i] not in repeate:
        repeate.append(x[i])
  return repeate
list1 = [10, 20, 30, 20, 20, 30, 40,  
         50, -20, 60, 60, -20, -20] 
print (Repeate(list1)) 
